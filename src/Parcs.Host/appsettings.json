{
  "Daemons": {
    "PreconfiguredInstances": [
      {
        "HostUrl": "172.17.0.2",
        "Port": 1111
      }
    ]
  },
  "Elasticsearch": {
    "BaseUrl": "http://elasticsearch:9200"
  },
  "FileSystem": {
    "BasePath": "/var/lib/storage"
  },
  "Hosting": {
    "Environment": "Any"
  },
  "JobOutput": {
    "Filename": "JobOutput{0}"
  },
  "JobTracking": {
    "MaximumActiveJobs": 10
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceBus": {
    "ConnectionString": "",
    "QueueName": "point-requested"
  },
  "HostTcp": {
    "Port": 2222
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "Parcs.Host"
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "source": "csharp"
        }
      }
    ]
  }
}