{
  "Node": {
    "Port": 1111
  },
  "FileSystem": {
    "BasePath": "/var/lib/storage"
  },
  "Hosting": {
    "Environment": "Any"
  },
  "Daemons": {
    "PreconfiguredInstances": [
      {
        "HostUrl": "172.17.0.2",
        "Port": 1111
      }
    ]
  },
  "Elasticsearch": {
    "BaseUrl": "http://elasticsearch:9200"
  },
  "HostApi": {
    "JobFailuresPath": "api/jobFailures"
  },
  "ServiceBus": {
    "ConnectionString": "",
    "QueueName": "point-requested"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "Parcs.Daemon"
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "source": "csharp"
        }
      }
    ]
  }
}